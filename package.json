{
  "name": "blog-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm exec vite build",
    "vercel-build": "npm exec vite build",
    "build:clean": "rm -rf dist && npm exec vite build",
    "build:gh-pages": "npm exec vite build -- --base=/blog-project/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "npm exec vite preview",
    "deploy": "npm run build:gh-pages && npm exec gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "tsparticles": "^2.12.0",
    "react-tsparticles": "^2.12.0",
    "framer-motion": "^10.0.1",
    "@heroicons/react": "^2.0.16"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.1.0",
    "vite": "4.5.14"
  }
}
